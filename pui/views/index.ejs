<!DOCTYPE html>
<html lang="en">
  <head>
      <link rel="stylesheet"  type="text/css" href="style.css">
      
      <script src="search.js"></script>
	<meta charset="utf-8">
	<title>Photo Network</title>
  </head>
  <h1>
 <img src="logo.png" alt="Smiley face"> 
	</h1>
  <body>
  <div class="wrapper">
      <input type="text" class="myInput" onkeyup="myFunction()" placeholder="Search for capture device">
      
      <form id="capture" method="POST" action="/assets">

	<input type="submit" value="Capture">
	<input type="text" class="myInput" name="capAmount" placeholder="Capture amount">


      </form>
      
      
      <table class="hoverTable" >
       
        <tr>
          <th>ID </th>
          <th>Client IP</th>
          <th>Total HB</th>
          <th>Time</th>
        </tr>
        	

	
	<% dbData.forEach((line) => {%>

        <tr>
          <td><%= line.id %></td>
          <td><%= line.client_ip %></td>
          <td><%= line.hb %></td>
          <td><%= line.time %></td>
        </tr>

        <% }) %>
	
      </table>
      </div>
	  </body>

  </html>


