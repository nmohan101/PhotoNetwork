<!DOCTYPE html>
<html lang="en">
  <head>
      <link rel="stylesheet"  type="text/css" href="style.css">
      
      <script src="search.js"></script>
	<meta charset="utf-8">
	<title>example</title>
  </head>
  <body>
      <input type="text" id="myInput" onkeyup="myFunction()" placeholder="Search for names..">
      
      <form id="capture" method="POST" action="/assets">

	<input type="submit" value="Capture">
	<input type="text" name="capAmount" placeholder="Capture amount">


      </form>
      
      
      <table class="hoverTable" id="hoverTable">
       <div class="tHead"> 
        <tr>
          <th>ID </th>
          <th>Client IP</th>
          <th>Total HB</th>
          <th>Time</th>
        </tr>
        <% dbData.forEach((line) => {%>
	</div>
	
        <tr>
          <td><%= line.id %></td>
          <td><%= line.client_ip %></td>
          <td><%= line.hb %></td>
          <td><%= line.time %></td>
        </tr>
        <% }) %>
      </table>
	  </body>

  </html>


